<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:paddingTop="50dp"
    tools:context=".presentation.OrderDetailActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_order_detail"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navigationIcon="?attr/homeAsUpIndicator">

        <com.google.android.material.textview.MaterialTextView
            style="@style/Text.Medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/waiting_for_payment"
            android:textColor="@color/base_black" />
    </androidx.appcompat.widget.Toolbar>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_order_detail">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tv_order_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/order_id_143740283"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/ll_timer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="28dp"
                android:background="@drawable/bg_timer_countdown"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/tv_order_id">

                <com.google.android.material.textview.MaterialTextView
                    style="@style/TextS.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="@string/complete_payment_in"
                    android:textColor="@color/white" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_timer"
                    style="@style/TextS.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="12dp"
                    android:background="@drawable/bg_text_timer"
                    android:gravity="center"
                    android:text="00:00:00"
                    android:textColor="@color/primary_500" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_depart_return"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="12dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/bg_depart_return"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@id/ll_timer">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_details_departure"
                        style="@style/H6.Semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="20dp"
                        android:text="@string/yia"
                        android:textColor="@color/white" />

                    <ImageView
                        android:id="@+id/iv_flight"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/img_line" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_details_arrival"
                        style="@style/H6.Semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:text="@string/cgk"
                        android:textColor="@color/white" />
                </LinearLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_depart"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="4dp"
                    android:background="@color/white"
                    android:padding="12dp">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_departure"
                        style="@style/Text.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/depart"
                        android:textColor="@color/primary_500"
                        android:textSize="16sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/img_departure_flight"
                        android:layout_width="24dp"
                        android:layout_height="12dp"
                        android:layout_marginTop="6dp"
                        android:layout_marginEnd="8dp"
                        android:src="@drawable/img_plane"
                        app:layout_constraintEnd_toStartOf="@id/tv_plane"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_plane"
                        style="@style/Text.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ga207"
                        android:textColor="@color/base_black"
                        android:textSize="16sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_departure_date"
                        style="@style/TextL.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:text="Fri, 19 Jan 2024"
                        android:textColor="@color/base_black"
                        android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_departure" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_departure_time"
                        style="@style/TextS.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="18dp"
                        android:text="08:00 AM"
                        android:textColor="@color/gray_500"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_plane" />

                    <View
                        android:id="@+id/v_line_depart"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/line_depart_return"
                        app:layout_constraintTop_toBottomOf="@id/tv_departure_time" />

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="Terminal 3 Domestic"
                        android:textColor="@color/base_black"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/v_line_depart" />

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/ic_arrow_right"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/v_line_depart" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_return"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="4dp"
                    android:background="@drawable/bg_return_order"
                    android:padding="12dp">

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_return"
                        style="@style/Text.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/return_"
                        android:textColor="@color/primary_500"
                        android:textSize="16sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/img_return_flight"
                        android:layout_width="24dp"
                        android:layout_height="12dp"
                        android:layout_marginTop="6dp"
                        android:layout_marginEnd="8dp"
                        android:src="@drawable/img_plane"
                        app:layout_constraintEnd_toStartOf="@id/tv_plane_return"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_plane_return"
                        style="@style/Text.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ga207"
                        android:textColor="@color/base_black"
                        android:textSize="16sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_return_date"
                        style="@style/TextL.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:text="Fri, 19 Jan 2024"
                        android:textColor="@color/base_black"
                        android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_return" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_return_time"
                        style="@style/TextS.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="18dp"
                        android:text="08:00 AM"
                        android:textColor="@color/gray_500"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_plane_return" />

                    <View
                        android:id="@+id/v_line_return"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/line_depart_return"
                        app:layout_constraintTop_toBottomOf="@id/tv_return_date" />

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="Terminal 3 Domestic"
                        android:textColor="@color/base_black"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/v_line_return" />

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/ic_arrow_right"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/v_line_return" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/tv_passenger_facilities"
                style="@style/Text.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:text="@string/passenger_facilities"
                android:textColor="@color/base_black"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/ll_depart_return" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_passenger_facilities"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/dimen_16"
                android:layout_marginTop="16dp"
                android:background="@drawable/bg_passenger_facilities"
                android:padding="@dimen/dimen_8"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_passenger_facilities">

                <com.google.android.material.chip.ChipGroup
                    android:id="@+id/cgPassengers"
                    android:layout_width="wrap_content"
                    android:layout_height="58dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:singleSelection="true">

                    <com.google.android.material.chip.Chip
                        android:id="@+id/chip_user_1"
                        style="@style/passengerOrderChip"
                        android:layout_width="112dp"
                        android:layout_height="60dp"
                        android:text="Bella Hadid"
                        app:chipCornerRadius="8dp"
                        android:checked="true"/>

                    <com.google.android.material.chip.Chip
                        android:id="@+id/chip_user_2"
                        style="@style/passengerOrderChip"
                        android:layout_width="112dp"
                        android:layout_height="60dp"
                        android:text="Jack Harris"
                        app:chipCornerRadius="8dp" />
                </com.google.android.material.chip.ChipGroup>

                <View
                    android:id="@+id/v_line_passenger_facilities"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:background="@drawable/line_passenger_facilities"
                    app:layout_constraintTop_toBottomOf="@id/cgPassengers" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_city_depart"
                    style="@style/Text.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="@string/yogyakarta"
                    android:textColor="@color/base_black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/v_line_passenger_facilities" />

                <ImageView
                    android:id="@+id/iv_arrow_right"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    android:src="@drawable/ef_ic_arrow_forward"
                    app:layout_constraintStart_toEndOf="@id/tv_city_depart"
                    app:layout_constraintTop_toBottomOf="@id/v_line_passenger_facilities"
                    app:tint="@color/primary_500" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_city_return"
                    style="@style/Text.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    android:text="@string/jakarta"
                    android:textColor="@color/base_black"
                    app:layout_constraintStart_toEndOf="@id/iv_arrow_right"
                    app:layout_constraintTop_toBottomOf="@id/v_line_passenger_facilities" />

                <TableLayout
                    android:id="@+id/table_addon_depart"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:stretchColumns="*"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_city_depart">

                    <TableRow>

                        <com.google.android.material.textview.MaterialTextView
                            style="@style/TextXS.Semibold"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_head_left"
                            android:paddingEnd="12dp"
                            android:text="@string/add_ons"
                            android:textColor="@color/white"
                            tools:ignore="RtlSymmetry" />

                        <com.google.android.material.textview.MaterialTextView
                            style="@style/TextXS.Semibold"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_head_right"
                            android:paddingEnd="12dp"
                            android:text="@string/evidence"
                            android:textColor="@color/white"
                            tools:ignore="RtlSymmetry" />

                    </TableRow>

                    <TableRow android:id="@+id/tr_extra_baggage">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_extra_baggage"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="@string/extra_baggage"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_baggage_mass"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="16 KG"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                    </TableRow>

                    <TableRow android:id="@+id/tr_meal">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_meal"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="@string/meal"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_total_meal"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="@string/_1_meal"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                    </TableRow>

                    <TableRow android:id="@+id/tr_flight_delay">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_flight_delay"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="@string/flight_delay"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_price_flight_delay"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="Insured"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                    </TableRow>

                    <TableRow android:id="@+id/tr_baggage_issurance">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_baggage_insurance"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="Baggage Insurance"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_price_baggage_insurance"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="Insured"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                    </TableRow>

                </TableLayout>

                <View
                    android:id="@+id/v_line_addon"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:background="@drawable/line_passenger_facilities"
                    app:layout_constraintTop_toBottomOf="@id/table_addon_depart" />


                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_city_retun_depart"
                    style="@style/Text.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="@string/jakarta"
                    android:textColor="@color/base_black"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/v_line_addon" />

                <ImageView
                    android:id="@+id/iv_arrow_right_return"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    android:src="@drawable/ef_ic_arrow_forward"
                    app:layout_constraintStart_toEndOf="@id/tv_city_retun_depart"
                    app:layout_constraintTop_toBottomOf="@id/v_line_addon"
                    app:tint="@color/primary_500" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_city_return_return"
                    style="@style/Text.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    android:text="@string/yogyakarta"
                    android:textColor="@color/base_black"
                    app:layout_constraintStart_toEndOf="@id/iv_arrow_right_return"
                    app:layout_constraintTop_toBottomOf="@id/v_line_addon" />

                <TableLayout
                    android:id="@+id/table_addon_return"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:stretchColumns="*"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_city_retun_depart">

                    <TableRow>

                        <com.google.android.material.textview.MaterialTextView
                            style="@style/TextXS.Semibold"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_head_left"
                            android:paddingEnd="12dp"
                            android:text="@string/add_ons"
                            android:textColor="@color/white"
                            tools:ignore="RtlSymmetry" />

                        <com.google.android.material.textview.MaterialTextView
                            style="@style/TextXS.Semibold"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_head_right"
                            android:paddingEnd="12dp"
                            android:text="@string/evidence"
                            android:textColor="@color/white"
                            tools:ignore="RtlSymmetry" />

                    </TableRow>

                    <TableRow android:id="@+id/tr_extra_baggage_return">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_extra_baggage_return"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="@string/extra_baggage"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_baggage_mass_return"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
                            android:paddingEnd="12dp"
                            android:text="16 KG"
                            android:textColor="@color/base_black"
                            tools:ignore="RtlSymmetry" />

                    </TableRow>

                    <TableRow android:id="@+id/tr_meal_return">

                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/tv_meal_return"
                            style="@style/TextXS.Regular"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@drawable/bg_table_body"
